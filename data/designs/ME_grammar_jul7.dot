digraph initialize {
  subgraph L {
    machine [require_label="machine"];
  }

  subgraph R {
    head [label="head"];
    body [label="body"];
    tail [label="tail"];
    head -> body [label="linkage"];
    body -> tail [label="linkage"];
  }
}

digraph append_body {
  subgraph L {
    tail [require_label="tail"];
    parent -> tail [id="parent_edges"];
  }

  subgraph R {
    tail [label="tail"];
    body [label="body"];
    parent -> body [id="parent_edges"];
    body -> tail [label="linkage"];
  }
}

digraph end_tail {
  subgraph L {
    tail [require_label="tail"];
    parent -> tail;
  }

  subgraph R {
    parent;
  }
}

digraph end_head {
  subgraph L {
    head [require_label="head"];
    head -> child;
  }

  subgraph R {
    child;
  }
}

